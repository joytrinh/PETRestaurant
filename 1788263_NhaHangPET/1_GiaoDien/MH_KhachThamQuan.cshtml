@{ 
    var DanhSachNhomHang = XL_LuuTru.DocDanhSachNhomHang();
    var DanhSachMatHang = XL_LuuTru.DocDanhSachMatHang();
    var KhachThamQuan = new XL_KhachThamQuan(); // Bien (ket qua)

    //Xu ly the hien: Tao chuoi HTML
    var ChuoiHTML_KhachThamQuan = XL_TheHien.TaoChuoiHTMLChaoKhachThamQuan(KhachThamQuan);

    var ChuoiHTML_ThongBao = "<br/> Chúng tôi hiện có " + DanhSachNhomHang.Count + " Nhóm hàng với " + DanhSachMatHang.Count + " Mặt hàng";

    var ChuoiHTML_DanhSachNhomHang = XL_TheHien.TaoChuoiHTMLDanhSachNhomHang(DanhSachNhomHang, DanhSachMatHang);
    var ChuoiHTML_DanhSachMatHang = XL_TheHien.TaoChuoiHTMLDanhSachMatHang(DanhSachMatHang);
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css" integrity="sha384-rwoIResjU2yc3z8GV/NPeZWAv56rSmLldC3R/AZzGRnGxQQKnKkoFVhFQhNUwEyJ" crossorigin="anonymous" />
</head>
<body style="font-family:Arial;">
    <div class="container-fluid">
        @Html.Raw(ChuoiHTML_KhachThamQuan)
        @Html.Raw(ChuoiHTML_ThongBao)
        @Html.Raw(ChuoiHTML_DanhSachNhomHang)
        @Html.Raw(ChuoiHTML_DanhSachMatHang)
    </div>
</body>
</html>
<script src="https://code.jquery.com/jquery-3.1.1.slim.min.js" integrity="sha384-A7FZj7v+d/sdmMqp/nOQwliLvUsJfDHW+k9Omg/a/EheAdgtzNs3hpfag6Ed950n" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb" crossorigin="anonymous"></script>
